{
	"name": "@vox1337/distinct",
	"description": "A utility library that provides functions to remove duplicate values from primitive and complex arrays.",
	"type": "module",
	"version": "0.1.1",
	"license": "MIT",
	"author": "Vladimir Aleksic",
	"maintainers": [
		{
			"name": "Vladimir Aleksic"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vladavoX/distinct.git"
	},
	"keywords": [
		"distinct",
		"distinctBy",
		"unique",
		"array",
		"deduplicate",
		"remove-duplicates",
		"utility",
		"typescript",
		"javascript",
		"library"
	],
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "bunup --format cjs,esm --exports",
		"dev": "bunup --watch",
		"test": "bun test",
		"biome": "biome check .",
		"biome:fix": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@types/bun": "latest",
		"@types/lodash-es": "^4.17.12",
		"bunup": "^0.16.10"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"lodash-es": "^4.17.22"
	},
	"engines": {
		"node": ">=20"
	}
}

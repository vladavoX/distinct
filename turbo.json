{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"//#biome": {},
		"//#biome:fix": { "cache": false },
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
